{
  "name": "bff-demo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "copy-resources": "ts-node scripts/copy-resources.ts",
    "build": "nest build && npm run copy-resources",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run build && node dist/main",
    "start:dev": "npm run copy-resources && tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "npm run copy-resources && tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "createTag": " git tag feature-20230313-2\ngit push origin feature-20230313-2",
    "docker": "docker build -t bff:v1 .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@altafid/nestjs-logging": "file:internal-package/altafid-nestjs-logging-1.0.6.tgz",
    "@google-cloud/opentelemetry-cloud-trace-exporter": "^2.0.0",
    "@metinseylan/nestjs-opentelemetry": "3.0.0",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/microservices": "9.4.3",
    "@nestjs/platform-express": "9.4.3",
    "@opentelemetry/api": "1.2.0",
    "@opentelemetry/core": "1.9.0",
    "@opentelemetry/exporter-zipkin": "1.9.0",
    "@opentelemetry/resources": "1.9.0",
    "@opentelemetry/sdk-trace-base": "1.9.0",
    "@opentelemetry/semantic-conventions": "1.0.0",
    "axios": "^1.3.4",
    "cache-manager": "4.1.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "^0.13.0",
    "cockatiel": "^2.0.2",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "qs": "^6.11.1",
    "redis": "4.3.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.3",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/npm": "^7.1.3",
    "@types/amqplib": "0.8.2",
    "@types/express": "4.17.13",
    "@types/jest": "29.5.11",
    "@types/multer": "^1.4.7",
    "@types/node": "18.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "@umijs/fabric": "^2.14.1",
    "cross-env": "^7.0.3",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "2.7.1",
    "semantic-release": "^17.4.7",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.20",
    "ts-loader": "9.3.1",
    "ts-morph": "15.1.0",
    "ts-node": "10.8.2",
    "ts-jest": "^29.1.1",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4",
    "webpack": "^5.0.0"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testEnvironment": "node",
    "testRegex": "(/test/.*|\\.(spec))\\.(ts)$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/git",
        {
          "assets": "false"
        }
      ]
    ]
  }
}
